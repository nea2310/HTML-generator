{
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "commonjs": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "ts": true
    },
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": [ "@typescript-eslint" ],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "globals": {
    "JQuery": "readonly",
    "$": "readonly",
    "NodeListOf": "readonly"
  }
}